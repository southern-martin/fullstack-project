  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "stop:all": "pkill -f 'react-scripts' && pkill -f 'npm start' && pkill -f 'python3 -m http.server' && lsof -ti:3000 | xargs kill -9 2>/dev/null || true && lsof -ti:3001 | xargs kill -9 2>/dev/null || true && echo 'All processes stopped successfully'",
    "restart": "npm run stop:all && sleep 2 && npm start",
    "restart:debug": "npm run stop:all && sleep 2 && DEBUG=* npm start",
    "start:debug": "DEBUG=* react-scripts start",
    "dev": "npm run restart"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "puppeteer": "^24.23.0"
  }
